{
    "version": 2,
    "builds":[
        {
            "src": "./index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["public/assets/NevveEnglish.pdf", "public/assets/NevveSpanish.pdf"]
            }
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}